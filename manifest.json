{
  "name": "STUDOCU Scraper",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["activeTab", "scripting"],
   "action": {
    "default_popup": "popup.html"
  },
  
 
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "host_permissions": ["<all_urls>"]
}
